{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/livros",
      "dest": "/books.json"
    },
    {
      "src": "/favoritos",
      "dest": "/favoritos.json"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
